{
  "config": {
    "update_multi": true
  },
  "i18n_header": {
    "zh_cn": {
      "title": {
        "tag": "plain_text",
        "content": "{{.title}}"
      },
      "subtitle": {
        "tag": "plain_text",
        "content": "{{.subtitle}}"
      },
      "template": "{{.title_color}}"
    }
  },
  "i18n_elements": {
    "zh_cn": [
      {{if .content_tips}}
      {
        "tag": "markdown",
        "content": "{{.content_tips}}",
        "text_align": "left",
        "text_size": "normal",
        "icon": {
          "tag": "standard_icon",
          "token": "warning_outlined",
          "color": "{{.title_color}}"
        }
      },
      {{end}}
      {
        "tag": "markdown",
        "content": "{{.content}}",
        "text_align": "left",
        "text_size": "normal_v2"
      }
    ]
  }
}